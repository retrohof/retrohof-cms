@using Volo.Abp.AspNetCore.Mvc.UI.Theme.Basic
@using Volo.Abp.Ui.Branding
@inject IAgileCmsBrandingProvider BrandingProvider

<div id="logo">
    <a asp-page="Index">
        <img class="logo-default" srcset="/themes/@BrandingProvider.AppName/images/logo.png, /themes/@BrandingProvider.AppName/images/logo@2x.png 2x" src="/themes/@BrandingProvider.AppName/images/logo@2x.png" alt="MD Wigmore Logo">
        <img class="logo-dark" srcset="/themes/@BrandingProvider.AppName/images/logo-dark.png, /themes/@BrandingProvider.AppName/images/logo-dark@2x.png 2x" src="/themes/@BrandingProvider.AppName/images/logo-dark@2x.png" alt="MD Wigmore Logo">
    </a>
</div>